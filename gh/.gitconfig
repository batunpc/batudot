[user]
	name = batunpc
	email = batuhnpc@gmail.com
[color]
	ui = true
[color "status"]
	changed = blue normal
	untracked = red normal
	added = magenta normal
	updated = green normal
	branch = yellow normal bold
	header = white normal bold
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	
[core]
	eol = lf
	autocrlf = false
	hooksPath = .husky
	editor = code --wait
[fsck]
	zeroPaddedFilemode = ignore
[fetch "fsck"]
	zeroPaddedFilemode = ignore
[receive "fsck"]
	zeroPaddedFilemode = ignore
[oh-my-zsh]
	lastVersion = 4fc25d5c0f378a7db180f283180e0c2e32714d21
[pull]
	rebase = false

[remote "origin"]
	url = https://github.com/glific/waffle_gcs
